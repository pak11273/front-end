{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": [
    "react-hooks",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "react-app",
    "prettier"
  ],
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "function-paren-newline": 0,
    "func-names": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-console": 1, 
    "no-underscore-dangle": 0,
    "prefer-destructuring": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "object-curly-newline": 0,
    "prettier/prettier": 2,
    "react-hooks/rules-of-hooks": 2, // Checks rules of Hooks
    "react-hooks/exhaustive-deps": 1, // Checks effect dependencies
    "react/jsx-filename-extension": 0,
    "semi": 2 // 0 means "off", 1 means "warning", 2 means "error"
  }
}